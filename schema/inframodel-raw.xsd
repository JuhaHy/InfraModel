	<xs:element name="Profile">
		<xs:annotation>
			<xs:documentation>A profile or long section</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="ProfSurf" minOccurs="0" maxOccurs="unbounded"/>
